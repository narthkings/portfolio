// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model experience {
  id              String @id @default(uuid())
  name_of_company String @default("")
  role            String @default("")
  start_date      String @default("")
  end_date        String @default("")
  content         String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  ord             Int    @default(1)
}

model contact {
  id      String @id @default(uuid())
  name    String @default("")
  email   String @default("")
  message String @default("")
}

model portfolioProject {
  id          String  @id @default(uuid())
  name        String  @default("")
  description String  @default("")
  projectUrl  String  @default("")
  badges      stack[] @relation("portfolioProject_badges")
}

model stack {
  id                           String             @id @default(uuid())
  name                         String             @default("")
  from_portfolioProject_badges portfolioProject[] @relation("portfolioProject_badges")
}